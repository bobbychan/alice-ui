{
  "name": "@alice-ui/react",
  "version": "1.0.2",
  "keywords": [
    "react",
    "react-component",
    "react-ui",
    "typescript"
  ],
  "homepage": "https://github.com/bobbychan/alice-ui#readme",
  "sideEffects": false,
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobbychan/alice-ui.git",
    "directory": "packages/react"
  },
  "scripts": {
    "build": "tsup src --dts && rimraf dist/**/*.stories.*",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast -- --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rimraf .turbo node_modules dist",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "framer-motion": ">=4.0.0",
    "react": ">=18"
  },
  "dependencies": {
    "@alice-ui/hooks": "workspace:*",
    "@alice-ui/icons": "workspace:*",
    "@alice-ui/react-utils": "workspace:*",
    "@alice-ui/shared-utils": "workspace:*",
    "@alice-ui/theme": "workspace:*"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^5.0.1"
  },
  "clean-package": "../../clean-package.config.json"
}
